{
  "name": "grips",
  "preferGlobal": true,
  "version": "0.2.0",
  "description": "grips templating engine",
  "main": "./node-grips.js",
  "bin": {
    "grips-build": "./bin/build",
    "grips": "./bin/grips"
  },
  "directories": {
    "example": "examples"
  },
  "optionalDependencies": {
    "uglify-js": "~1.3.3"
  },
  "devDependencies": {},
  "scripts": {
    "install": "bin/build --all --minify",
    "update": "bin/build --all --minify",
    "preuninstall": "rm deploy/* node-grips.js node-grips-debug.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getify/grips.git"
  },
  "keywords": [
    "javascript",
    "templating",
    "templates",
  ],
  "author": "Kyle Simpson <getify@gmail.com>",
  "license": "MIT"
}
